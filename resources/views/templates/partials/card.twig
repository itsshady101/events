<div class="card">
	<img src="{% block image %}{% endblock %}" alt="" class="card-img-top img-responsive">
	<div class="card-block">
		<h4 class="card-title">{% card_title %}{% endblock %}</h4>
		<p class="lead">{% block date %}{% endblock %} | {% block location %}{% endblock %}</p>
		<p class="card-text">{% block description %}{% endblock %}</p>
		<a href="#" class="btn btn-primary">Know More</a>
	</div>
</div>